{
	"extends": "astro/tsconfigs/strict",
	"include": ["alchemy.run.ts", "types/**/*.ts", ".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"types": ["@cloudflare/workers-types", "./types/env.d.ts"],
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@server/*": ["../server/src/*"],
			"server/alchemy": ["../server/alchemy.run.ts"]
		},
		
	}
}
