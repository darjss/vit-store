{
	"extends": "astro/tsconfigs/strict",
	"include": [
		"alchemy.run.ts",
		"../../env.ts",
		"types/**/*.ts",
		".astro/types.d.ts",
		"**/*",
		"src/env.d.ts"
	],
	"exclude": ["dist"],
	"compilerOptions": {
		"types": ["@cloudflare/workers-types"],
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@server/*": ["../server/src/*"],
			"server/alchemy": ["../server/alchemy.run.ts"]
		}
	}
}
